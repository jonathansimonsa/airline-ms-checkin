					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"\r",
							"    var data = JSON.parse(responseBody);\r",
							"    if (data.length > 0) {\r",
							"        pm.environment.unset(\"checkInId\");\r",
							"        var i = _.random(0, data.length - 1);\r",
							"        postman.setEnvironmentVariable(\"reservaId\", data[i].id);\r",
							"        postman.setEnvironmentVariable(\"esAltaPrioridad\", _.random(0, 1));\r",
							"        var cantEquipaje = _.random(0, 5);\r",
							"        var cont = 0;\r",
							"        var detalleEquipaje = [];\r",
							"        while (cont < cantEquipaje) {\r",
							"            detalleEquipaje.push({\r",
							"                \"descripcion\": \"Maleta\",\r",
							"                \"peso\": _.random(1, 45),\r",
							"                \"esFragil\": _.random(0, 1)\r",
							"            });\r",
							"            cont++;\r",
							"        }\r",
							"        postman.setEnvironmentVariable(\"detalleEquipaje\", JSON.stringify(detalleEquipaje));\r",
							"    }\r",
							"\r",
							"});\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
